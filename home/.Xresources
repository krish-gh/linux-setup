Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.vt100.locale: false
*.vt100.utf8: true
*.vt100.scrollBar: false
*.vt100.rightScrollBar: false
*.vt100.scrollbar.width: 1
*.vt100.geometry: 100x32
*.vt100.faceName: JetBrainsMono Nerd Font:size=12:antialias=false
*.vt100.cursorBlink: true
*.vt100.TrimSelection: true
*.vt100.translations: #override \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

! special
*background: #1E1E2E
*foreground: #CDD6F4
*cursorColor: #F5E0DC
*scrollbar.foreground: #CDD6F4
*scrollbar.background: #1E1E2E

! black
*color0: #45475A
*color8: #585B70

! red
*color1: #F38BA8
*color9: #F38BA8

! green
*color2: #A6E3A1
*color10: #A6E3A1

! yellow
*color3: #F9E2AF
*color11: #F9E2AF

! blue
*color4: #89B4FA
*color12: #89B4FA

! magenta
*color5: #F5C2E7
*color13: #F5C2E7

! cyan
*color6: #94E2D5
*color14: #94E2D5

! white
*color7: #BAC2DE
*color15: #A6ADC8
