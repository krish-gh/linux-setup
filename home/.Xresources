Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*.vt100.locale: false
*.vt100.utf8: true
*.vt100.scrollBar: false
*.vt100.rightScrollBar: false
*.vt100.scrollbar.width: 1
*.vt100.geometry: 100x32
*.vt100.faceName: JetBrainsMono Nerd Font:size=12:antialias=false
*.vt100.cursorBlink: true
*.vt100.TrimSelection: true
*.vt100.translations: #override \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

! special
*.vt100.background: #1E1E2E
*.vt100.foreground: #CDD6F4
*.vt100.cursorColor: #F5E0DC
*.vt100.scrollbar.foreground: #CDD6F4
*.vt100.scrollbar.background: #1E1E2E

! black
*.vt100.color0: #45475A
*.vt100.color8: #585B70

! red
*.vt100.color1: #F38BA8
*.vt100.color9: #F38BA8

! green
*.vt100.color2: #A6E3A1
*.vt100.color10: #A6E3A1

! yellow
*.vt100.color3: #F9E2AF
*.vt100.color11: #F9E2AF

! blue
*.vt100.color4: #89B4FA
*.vt100.color12: #89B4FA

! magenta
*.vt100.color5: #F5C2E7
*.vt100.color13: #F5C2E7

! cyan
*.vt100.color6: #94E2D5
*.vt100.color14: #94E2D5

! white
*.vt100.color7: #BAC2DE
*.vt100.color15: #A6ADC8
